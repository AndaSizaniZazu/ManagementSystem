@using MudBlazor
<MudPaper Width="250px" Class="d-inline-flex py-3" Elevation="0">
    <MudNavMenu Class="mud-width-full">
        <MudText Typo="Typo.h6" Class="px-4">Pages</MudText>
        <MudDivider Class="my-2" />
        <MudNavLink Href="/" Icon="@Icons.Material.Filled.Dashboard">Home</MudNavLink>
        <MudNavGroup Title="Actions" Icon="@Icons.Material.Filled.AdminPanelSettings" Expanded="true">
            <MudNavLink Href="/addcourse" Icon="@Icons.Material.Filled.Book" Match="NavLinkMatch.Prefix">Add a Course</MudNavLink>
            <MudNavLink Href="/courselist" Icon="@Icons.Material.Filled.ViewList" Match="NavLinkMatch.Prefix">Course List</MudNavLink>
            <MudNavLink Href="/addfacilitator" Icon="@Icons.Material.Filled.Person" Match="NavLinkMatch.Prefix">Add a Facilitator</MudNavLink>
            <MudNavLink Href="/facilitatorlist" Icon="@Icons.Material.Filled.People" Match="NavLinkMatch.Prefix">Facilitator List</MudNavLink>
            <MudNavLink Href="/addclass" Icon="@Icons.Material.Filled.Abc" Match="NavLinkMatch.Prefix">Add a Class</MudNavLink>
            <MudNavLink Href="/classlist" Icon="@Icons.Material.Filled.Abc" Match="NavLinkMatch.Prefix">Class List</MudNavLink>
            <MudNavLink Href="/addstudent" Icon="@Icons.Material.Filled.School" Match="NavLinkMatch.Prefix">Add a Student</MudNavLink>
        </MudNavGroup>
            
    </MudNavMenu>
</MudPaper>